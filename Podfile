# Uncomment the next line to define a global platform for your project
 platform :ios, '9.0'

target 'AppLoginWithFirebase' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
pod 'Firebase/Core'
pod 'Firebase/Auth'
pod 'Firebase/Storage'	
pod 'Firebase/Database'
pod 'SwiftSoup'
pod 'GoogleSignIn'
  # Pods for AppLoginWithFirebase

  target 'AppLoginWithFirebaseTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AppLoginWithFirebaseUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
